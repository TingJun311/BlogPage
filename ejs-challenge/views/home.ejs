
<head>
    <%- include("partials/header") -%>
        
        <h1 class="mt-3">Home</h1>
        <p><%= content %></p>

        <% postContent.forEach((i) => { %>
            <div>
                <a href=""><h2><%= i.title %></h2></a>
                <p><%= i.body.substring(0, length) + "..."; %> <a href="/posts/<%= i.title %>">Read More</a></p>
            </div>
        <% }); %>
    <%- include("partials/footer") -%>
</html>